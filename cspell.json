    "ignoreRegExpList": [
        // RST directives
        "(?::[a-z]+:|\\.\\. [a-z]+::)",
        // CLI options
        "--[a-z0-9-]+",
        // Defines in command line
        " -D[^ ]+",
        // GH actions
        "uses: [^\\s]+/[^\\s]+@[^\\s]+",
        // Jinja2 instructions
        "\\{%\\s*[^\\s]+\\s*%\\}"
    ],
    "import": [],
    "languageSettings": [
        {
            "languageId": "python",
            "ignoreRegExpList": [
                // Python function calls with package
                "[a-z0-9]+\\.[a-z0-9]+(?:\\(| =)",
                // Python function call assignments
                "= [a-z0-9]+\\(",
                // Python named arguments
                "(?:\\(| )[a-z0-9]+=",
                // Regular expressions in python
                "\\br\\\".+\\\""
            ]
        },
        {
            "languageId": "xml",
            "ignoreRegExpList": [
                // Tag names
                "<[^ >]+",
                // cspell:disable
                // Clover attributes
                " (?:covered[^=]+|ncloc)="
                // cspell:enable
            ]
        },
        {
            "languageId": "css",
            "ignoreWords": [
                // cspell:disable
                "functionslist",
                "transy"
                // cspell:enable
            ]
        }
    ]
